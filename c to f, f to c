#include<iostream>
using namespace std;
float ctof(float c)
{
	return (c*9/5)+32;
}
float ftoc(float f)
{
	return (f-32)*5/9;
}
int main()
{
	int temp;
	cout<<"enter temperature";
	cin>>temp;
	cout<<temp<<"c="<<ctof(temp)<<"f\n";
	cout<<"enter temperature";
	cin>>temp;
	cout<<temp<<"f="<<ftoc(temp)<<"c\n";
	return 0;
}
